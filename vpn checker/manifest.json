{
	"manifest_version": 2,
	"name": "IPchexmex",
	"description": "Checks to see if you are in a country that allows Bitmex trading",
	"version": "1.0",
	"author": "isaackoz",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon148.png"
	},
	"content_scripts": [
	{
		"matches": [
			"https://www.bitmex.com/*"
		],
		"js": [
			"jquery.min.js",
			"content.js"
		],
		"run_at": "document_end"
	}
	]
}